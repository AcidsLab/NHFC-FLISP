  <DataSets>
    <DataSet Name="PaymentReq">
      <Query>
        <DataSourceName>CRM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PersonId">
            <Value>=Parameters!PersonId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
DECLARE @LoanId int = (select Oppo_OpportunityId from Opportunity where oppo_deleted IS Null AND oppo_primarypersonid = @PersonId)

SELECT 
	CASE 
		WHEN oppo_AttorneyRequisition = 'y' THEN PaidToAttorneyAmount
		ELSE TransferToBondAmount
	END as Amount,
	CASE 
		WHEN oppo_AttorneyRequisition = 'y' THEN oppo_AttorneyAccountNumber
		ELSE pers_BondAccountNumber
	END AS AccountNumber,
	CASE 
		WHEN oppo_AttorneyRequisition = 'y' THEN oppo_AttorneyBank
		ELSE pers_BankName
	END AS BankName,
	CASE 
		WHEN oppo_AttorneyRequisition = 'y' THEN oppo_AttorneyBranchCode
		ELSE pers_BranchCode
	END AS BranchCode,
	CASE 
		WHEN oppo_AttorneyRequisition = 'y' THEN oppo_AttorneyName
		ELSE (Pers_LastName + ' ' + pers_Initials)
	END AS AccountHolder,
	/*
	CASE
		WHEN oppo_AttorneyRequisition = 'y' AND 1 != 1 THEN (oppo_AttorneyName + '	Client ID : ' + pers_idnumber)
		ELSE (Pers_LastName + ' ' + pers_Initials + ' : ' + pers_idnumber)
	END*/
	(ISNULL(Pers_LastName, '') + ' ' + ISNULL(pers_Initials, '') + ' : ' + ISNULL(pers_idnumber, '')) AS ReferenceNumber,
	addr_ErfNo AS ErfNumber,
	addr_Province AS Province,
	Capt_US AS ProvinceDescription, 
	
	CASE 
		WHEN oppo_AttorneyRequisition = 'y' THEN dbo.Currency_ToWords(PaidToAttorneyAmount)
		ELSE dbo.Currency_ToWords(TransferToBondAmount)
	END AS AmountAsWords,
	PurchaseAmount, 
	ApprovedAmount,
	ShortFallAmount,
	AllreadyPaidAmount,
	AfterDepositAmount,
	SubsidyAmount,
	PaidToAttorneyAmount,
	TransferToBondAmount,
	FinalShootFallAmount


FROM Person		INNER JOIN Opportunity on Pers_PersonId = Oppo_PrimaryPersonId
				INNER JOIN Company on Comp_CompanyId = Oppo_PrimaryCompanyId
				LEFT OUTER JOIN (	select * 
									From Address INNER JOIN Custom_Captions on Capt_Code = addr_Province 
									where	Capt_Family = 'addr_Province'
											and Addr_Deleted is null
											and Capt_Deleted is null
								)Address on Addr_AddressId = Pers_PrimaryAddressId
				CROSS APPLY fn_GetCalculatedAmounts(oppo_opportunityid) 


WHERE	Pers_PersonId = @PersonId
		AND Oppo_Deleted is null
		AND Pers_Deleted is null
		AND oppo_subsidyfinal IS NOT NULL</CommandText>
      </Query>
